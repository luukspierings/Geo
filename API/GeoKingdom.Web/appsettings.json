{
  "ConnectionStrings": {
    "Default": "Server=(localdb)\\mssqllocaldb;Database=GeoKingdom_v2;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "Database": {
    "UseInMemory": false
  },

  "Map": {
    "MetersPerTile": 1,
    "ChunkRenderDistance": 1,
    "TilesPerChunkDimension": 16,
    "ResourceDistribution": {
      "Trees": {
        "Zoom": 4.0,
        "Percentage": 5
      }

    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
